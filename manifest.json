{
  "manifest_version": 3,
  "name": "Metaculus Percentage Replacement",
  "description": "Replace the slider bar with an input field on Metaculus",
  "version": "1.0",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["percent.js"],
      "matches": [
        "https://www.metaculus.com/questions/*"
      ]
    }
  ]
}
